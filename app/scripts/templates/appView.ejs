<section class='content row'>
  <div class='side-content col-md-4'>
    <div id='existingReminders'>
      <table class='table table-bordered table-hover'>
        <tbody>
        </tbody>
      </table>
    </div>
  </div>
  <div class='main-content col-md-8'>
    <form role='form' id='newReminder'>
      <div class="form-group">
        <label for="description">Description</label>
        <input type='text' class='form-control' id='description' placeholder='Remind me to do this.'>
      </div>
      <div class="form-group">
        <label for="endTime">End Time</label>
        <input type='time' class='form-control' id='endTime' placeholder='23:40 10/29/14'>
      </div>
      <button type='submit' class='btn-default'>Submit</button>
    </form>
  </div>
</section>
